{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:profile": {
      "dependsOn": ["^build:profile"],
      "outputs": ["dist/**"]
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "publish": {
      "dependsOn": ["build", "test", "lint"],
      "outputs": []
    }
  }
}
