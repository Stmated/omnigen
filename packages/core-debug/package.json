{
  "name": "@omnigen-org/core-debug",
  "version": "0.0.5",
  "private": false,
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "rm -r ./dist && tsc --project ./tsconfig.build.json",
    "publish": "pnpm publish --access public --no-git-checks=true"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@omnigen/config-typescript": "workspace:*",
    "@omnigen/eslint-config-custom": "workspace:*",
    "@types/debug": "^4.1.12"
  }
}
