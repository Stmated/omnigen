{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "amd",
    "moduleResolution": "Node",

    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "sourceMap": false,
    "skipLibCheck": true,

    "outFile": "dist/cli.js",
    "removeComments": true
  },
  "files": [
    "./src/cli.ts"
  ]
}
