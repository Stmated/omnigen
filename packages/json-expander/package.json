{
  "name": "@omnigen-org/json-expander",
  "version": "0.0.22",
  "private": false,
  "license": "MIT",
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "json-expand": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "rm -r ./dist && tsc --project ./tsconfig.build.json",
    "publish": "pnpm publish --access public --no-git-checks=true"
  },
  "dependencies": {
    "@omnigen-org/core-debug": "workspace:*",
    "commander": "^12.1.0",
    "@commander-js/extra-typings": "^12.1.0"
  },
  "peerDependencies": {
    "change-case": "^5.4.4",
    "json-pointer": "^0.6.2",
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@omnigen/config-typescript": "workspace:*",
    "@omnigen/eslint-config-custom": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/json-pointer": "^1.0.34",
    "json-stringify-pretty-compact": "^4.0.0"
  }
}
