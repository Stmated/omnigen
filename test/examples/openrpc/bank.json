{
  "openrpc": "1.2.4",
  "info": {
    "title": "Bank",
    "version": "1.0.0"
  },
  "x-omnigen": {
    "jsonRpcPropertyName": "version",
    "jsonRpcRequestVersion": "1.1",
    "jsonRpcIdIncluded": false
  },
  "servers": [
    {
      "url": "http://localhost:8080"
    }
  ],
  "methods": [
    {
      "$ref": "#/components/methods/AccountLedgerMethod"
    }
  ],
  "components": {
    "methods": {
      "AccountLedgerMethod": {
        "name": "AccountLedger",
        "summary": "",
        "params": [
          {
            "$ref": "#/components/contentDescriptors/ParamSignature"
          },
          {
            "$ref": "#/components/contentDescriptors/ParamUUID"
          },
          {
            "name": "Data",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/AccountLedgerRequestData"
            }
          }
        ],
        "result": {
          "name": "AccountLedgerResponse",
          "schema": {
            "$ref": "#/components/schemas/AccountLedgerResponseData"
          }
        }
      }
    },
    "contentDescriptors": {
      "ParamSignature": {
        "name": "Signature",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "ParamUUID": {
        "name": "UUID",
        "required": true,
        "schema": {
          "type": "string"
        }
      }
    },
    "schemas": {
      "AbstractSignedRequestParams": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
        }
      },
      "AbstractToTrustlyRequestParamsData": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "Username": {
            "type": "string"
          },
          "Password": {
            "type": "string"
          }
        }
      },
      "AbstractResponseResultData": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "signature": {
            "type": "string"
          },
          "method": {
            "type": "string"
          },
          "uuid": {
            "type": "string"
          }
        }
      },
      "AccountLedgerRequestData": {
        "type": "object",
        "additionalProperties": false,
        "allOf": [
          {
            "$ref": "#/components/schemas/AbstractToTrustlyRequestParamsData"
          }
        ],
        "properties": {
          "FromDate": {
            "type": "string"
          },
          "ToDate": {
            "type": "string"
          },
          "Currency": {
            "type": "string"
          },
          "Attributes": {
            "$ref": "#/components/schemas/EmptyRequestParamsDataAttributes"
          }
        },
        "required": [
          "FromDate",
          "ToDate",
          "Currency"
        ]
      },
      "EmptyRequestParamsDataAttributes": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
        }
      },
      "AccountLedgerResponseData": {
        "additionalProperties": false,
        "allOf": [
          {
            "$ref": "#/components/schemas/AbstractResponseResultData"
          },
          {
            "properties": {
              "data": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/AccountLedgerResponseDataEntry"
                }
              }
            }
          }
        ]
      },
      "AccountLedgerResponseDataEntry": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "userid": {
            "description": "Your userid in our system.",
            "type": "string",
            "x-field-name": "user_id",
            "x-property-name": "UserId"
          },
          "datestamp": {
            "description": "The datestamp for when this ledger row affected your balance in our system.",
            "type": "string"
          },
          "orderid": {
            "description": "The globally unique OrderID that resulted in this ledger record.",
            "type": "string"
          },
          "accountname": {
            "description": "The name of the bookkeeping account this ledger record belongs to.",
            "type": "string"
          },
          "messageid": {
            "description": "Your unique MessageID that you used to create the order that resulted in this ledger record.",
            "type": "string"
          },
          "transactiontype": {
            "description": "A human friendly description of this ledger record.",
            "type": "string"
          },
          "currency": {
            "description": "The currency of the amount in this ledger record.",
            "type": "string"
          },
          "amount": {
            "description": "The amount your balance in our system was affected with due to this ledger record. May contain a lot of decimals.",
            "type": "string"
          },
          "gluepayid": {
            "description": "An ID meaning different things for different payment methods, you probably don't need this data.",
            "type": "string"
          }
        }
      },
      "AbstractFromTrustlyRequestParamsData": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "something": {
            "$ref": "#/components/schemas/repository"
          }
        }
      },
      "AbstractRequestParamsData": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
        }
      },
      "AbstractRequestParamsDataAttributes": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
        }
      }
    }
  }
}
