{
  "name": "omnigen",
  "packageManager": "pnpm@7.26.3",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "test": "FORCE_COLOR=1 turbo run test --continue",
    "test:watch": "FORCE_COLOR=1 turbo run test:watch",
    "lint": "turbo run lint",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "sync:prepare": "syncpack list-mismatches",
    "sync:execute": "syncpack fix-mismatches"
  },
  "syncpack": {
    "versionGroups": []
  },
  "devDependencies": {
    "@omnigen/eslint-config-custom": "0.0.0",
    "prettier": "^2.8.4",
    "turbo": "^1.8.3",
    "syncpack": "^9.8.4",
    "pino-pretty": "^10.0.0"
  }
}
