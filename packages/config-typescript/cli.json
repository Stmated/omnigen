{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "build",
  "extends": "./build.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationMap": false,
    "inlineSources": false,
    "sourceMap": false,
    "skipLibCheck": true,
    "noEmit": false,
    "allowImportingTsExtensions": false
  }
}
